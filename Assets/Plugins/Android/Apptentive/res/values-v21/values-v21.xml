<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Apptentive.MessageCenterMessageFrame">
        <item name="android:paddingTop">@dimen/apptentive_card_view_gap_vertical</item>
        <item name="android:paddingBottom">@dimen/apptentive_card_view_gap_vertical</item>
        <item name="android:paddingLeft">@dimen/apptentive_message_center_message_list_margin_side</item>
        <item name="android:paddingRight">@dimen/apptentive_message_center_message_list_margin_side</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="Apptentive.Style.Widget.Button.Colored" parent="Widget.AppCompat.Button.Colored">
        <item name="android:fontFamily">?apptentiveFontFamilyButtonColored</item>
    </style>
    <style name="Apptentive.TextAppearance.Body1" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:fontFamily">?attr/apptentiveFontFamilyBody1</item>
        <item name="android:textSize">?attr/apptentiveTextSizeBody1</item>
    </style>
    <style name="Apptentive.TextAppearance.Body2" parent="@style/TextAppearance.AppCompat.Body2">
        <item name="android:fontFamily">?attr/apptentiveFontFamilyBody2</item>
        <item name="android:textSize">?attr/apptentiveTextSizeBody2</item>
    </style>
    <style name="Apptentive.TextAppearance.Caption" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:fontFamily">?attr/apptentiveFontFamilyCaption</item>
        <item name="android:textSize">?attr/apptentiveTextSizeCaption</item>
    </style>
    <style name="Apptentive.TextAppearance.Headline" parent="@style/TextAppearance.AppCompat.Headline">
        <item name="android:fontFamily">?attr/apptentiveFontFamilyHeadline</item>
        <item name="android:textSize">?attr/apptentiveTextSizeHeadline</item>
    </style>
    <style name="Apptentive.TextAppearance.Subhead" parent="@style/TextAppearance.AppCompat.Subhead">
        <item name="android:fontFamily">?attr/apptentiveFontFamilySubhead</item>
        <item name="android:textSize">?attr/apptentiveTextSizeSubhead</item>
    </style>
    <style name="Apptentive.TextAppearance.Subhead.Inverse" parent="@style/TextAppearance.AppCompat.Subhead.Inverse">
        <item name="android:fontFamily">?attr/apptentiveFontFamilySubhead</item>
        <item name="android:textSize">?attr/apptentiveTextSizeSubhead</item>
    </style>
    <style name="Apptentive.TextAppearance.Subhead.Secondary">
        <item name="android:fontFamily">?attr/apptentiveFontFamilySubhead</item>
        <item name="android:textSize">?attr/apptentiveTextSizeSubhead</item>
        <item name="android:textColor">?android:attr/textColorSecondary</item>
    </style>
    <style name="Apptentive.TextAppearance.Title" parent="@style/TextAppearance.AppCompat.Title">
        <item name="android:fontFamily">?attr/apptentiveFontFamilyTitle</item>
        <item name="android:textSize">?attr/apptentiveTextSizeTitle</item>
    </style>
    <style name="Apptentive.TextAppearance.Toolbar" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:fontFamily">?attr/apptentiveFontFamilyToolbar</item>
    </style>
    <style name="Apptentive.Widget.Button.Borderless" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:fontFamily">?apptentiveFontFamilyButtonBorderless</item>
        <item name="android:textColor">?attr/apptentiveInteractionButtonColorDefault</item>
        <item name="android:textSize">?attr/apptentiveTextSizeButtonBorderless</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:minWidth">36dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
    </style>
    <style name="Apptentive.Widget.CheckBox" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:fontFamily">?attr/apptentiveFontFamilyBody1</item>
        <item name="android:textSize">?attr/apptentiveTextSizeBody1</item>
    </style>
    <style name="Apptentive.Widget.RadioButton" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:fontFamily">?attr/apptentiveFontFamilyBody1</item>
        <item name="android:textSize">?attr/apptentiveTextSizeBody1</item>
    </style>
    <style name="ApptentiveTheme.Base.Versioned.TranslucentStatus">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
</resources>